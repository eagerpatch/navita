{
  "name": "@navita/vite-plugin",
  "version": "1.0.1",
  "description": "Navita Vite Plugin",
  "keywords": [
    "vite",
    "css-in-js",
    "navita"
  ],
  "private": false,
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    },
    "./remix": {
      "import": "./dist/remix.mjs",
      "require": "./dist/remix.cjs",
      "types": "./src/remix.ts"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false
  },
  "scripts": {
    "build": "build",
    "dev": "build --dev",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@navita/core": "workspace:*",
    "@navita/css": "workspace:*"
  },
  "devDependencies": {
    "vite": "5.4.8"
  },
  "license": "MIT",
  "author": "Eagerpatch",
  "repository": {
    "type": "git",
    "url": "https://github.com/eagerpatch/navita.git",
    "directory": "packages/vite-plugin"
  }
}
